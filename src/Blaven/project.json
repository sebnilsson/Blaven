{
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [
          "DEBUG",
          "TRACE"
        ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [
          "RELEASE",
          "TRACE"
        ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "NETSTANDARD1_3",
          "PORTABLE"
        ]
      },
      "dependencies": {
        "System.Collections.Concurrent": "4.0.12",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Globalization.Extensions": "4.0.1",
        "System.Linq.Queryable": "4.0.1",
        "System.Text.RegularExpressions": "4.1.0",
        "System.Threading": "4.0.11"
      }
    }
  }
}